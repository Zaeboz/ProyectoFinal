          <TableColumn prefWidth="132.0" text="Categoria" />
        </columns>
      </TableView>
      <Button fx:id="crearCategoriaButton" layoutX="135.0" layoutY="14.0" mnemonicParsing="false" onAction="#lanzarVistaCrearCategoria" text="Crear categoria" />
   </children>
</AnchorPane>
