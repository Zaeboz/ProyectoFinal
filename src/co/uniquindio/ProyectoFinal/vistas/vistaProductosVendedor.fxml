      <TableView fx:id="productosTableView" layoutX="200.0" layoutY="72.0" prefHeight="313.0" prefWidth="200.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="200.0" AnchorPane.topAnchor="72.0">
