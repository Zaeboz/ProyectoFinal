          <TableColumn prefWidth="199.0" text="Nombre" />
        </columns>
      </TableView>
      <TableView fx:id="categoria" layoutX="69.0" layoutY="72.0" prefHeight="313.0" prefWidth="132.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="69.0" AnchorPane.topAnchor="72.0">
